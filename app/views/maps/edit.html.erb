<h1>Editing map</h1>

<% form_for(@map) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :svgdata%><br />
    <%= f.text_area :svgdata%>
  </p>
  <p>
    <%= f.label :width%><br />
    <%= f.text_field :width%>
  </p>
  <p>
    <%= f.label :height%><br />
    <%= f.text_field :height%>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @map %> |
<%= link_to 'Back', maps_path %>
